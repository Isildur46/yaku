{
  "name": "yaku",
  "version": "0.12.0",
  "description": "A light-weight ES6 Promises/A+ implementation that doesn't hurt.",
  "main": "lib/yaku.js",
  "typings": "lib/yaku.d.ts",
  "scripts": {
    "no": "no",
    "test": "no test",
    "prepublish": "no clean build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ysmood/yaku.git"
  },
  "keywords": [
    "light-weight",
    "es6",
    "promise",
    "performance",
    "promises",
    "promises-a",
    "promises-aplus",
    "async",
    "await",
    "deferred",
    "deferreds",
    "future",
    "flow control"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/yaku/issues"
  },
  "homepage": "https://github.com/ysmood/yaku",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "bluebird": "3.0.6",
    "es6-promise": "3.0.2",
    "eslint": "1.10.3",
    "junit": "1.1.1",
    "mocha": "2.3.4",
    "nokit": "0.16.15",
    "promises-aplus-tests": "*",
    "promises-es6-tests": "*",
    "q": "1.4.1",
    "uglify-js": "2.6.1",
    "webpack": "1.12.9"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        4
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "quotes": [
        2,
        "double"
      ],
      "no-cond-assign": 0,
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ]
    }
  }
}
